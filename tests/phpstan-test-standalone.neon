parameters:
    level: 0
    paths:
        - fixtures
    parallel:
        jobSize: 999999
        maximumNumberOfProcesses: 1

services:
    -
        class: ShadowCastiel\PHPStan\TestAttributes\TestAttributesExtension
        tags:
            - phpstan.broker.dynamicMethodReturnTypeExtension
            - phpstan.broker.dynamicStaticMethodReturnTypeExtension
            - phpstan.broker.dynamicFunctionReturnTypeExtension

    -
        class: ShadowCastiel\PHPStan\TestAttributes\Rule\TestAttributeRule
        arguments:
            checkedClassPatterns:
                - 'Tests\\Fixtures\\*Service'
                - 'Tests\\Fixtures\\ValidService'
                - 'Tests\\Fixtures\\MissingAttributeService'
                - 'Tests\\Fixtures\\MissingFilePathService'
                - 'Tests\\Fixtures\\InvalidFilePathService'
                - 'Tests\\Fixtures\\PathFormatsService'
            excludedMethods: []
        tags:
            - phpstan.rules.rule

